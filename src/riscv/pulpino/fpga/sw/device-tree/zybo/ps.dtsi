            } ;
        } ;
        ps7_spi_0: ps7-spi@e0006000 {
            clock-names = "ref_clk", "pclk";
            clocks = <&clkc 25>, <&clkc 34>;
            compatible = "cdns,spi-r1p6";
            interrupt-parent = <&ps7_scugic_0>;
            interrupts = <0 26 4>;
            is-decoded-cs = <0>;
            num-cs = <3>;
            reg = <0xe0006000 0x1000>;
            xlnx,has-ss0 = <0x1>;
            xlnx,has-ss1 = <0x1>;
            xlnx,has-ss2 = <0x1>;
            #address-cells=<1>;
            #size-cells=<0>;
            status = "okay";
            device@0 {
              compatible = "spidev";
              reg = <0>;  // Chip select 0
              spi-max-frequency = <500000>;
              spi-cpol = <0x0>;
              spi-cpha = <0x0>;
            };
