        ps7_scugic_0: ps7-scugic@f8f00100 {
            #address-cells = <2>;
            #interrupt-cells = <3>;
            #size-cells = <1>;
            compatible = "arm,cortex-a9-gic", "arm,gic";
            interrupt-controller ;
            num_cpus = <2>;
            num_interrupts = <96>;
            reg = <0xF8F01000 0x1000 0xF8F00100 0x100>;
